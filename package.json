{
  "name": "chat-cursor",
  "displayName": "chat-cursor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chatCursor.generateCode",
        "title": "Chat Cursor: Generate Code"
      },
      {
        "command": "chatCursor.updateCode",
        "title": "Chat Cursor: Update Code"
      },
      {
        "command": "chatCursor.createFile",
        "title": "Chat Cursor: Create File"
      },
      {
        "command": "chatCursor.deleteFile",
        "title": "Chat Cursor: Delete File"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "@google/generative-ai": "^0.23.0",
    "axios": "^1.8.1",
    "dotenv": "^16.4.7"
  }
}
